# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://appwrite.your-domain.com/v1
VITE_APPWRITE_PROJECT_ID=your_appwrite_project_id
VITE_APPWRITE_DATABASE_ID=your_database_id

# Collection IDs
VITE_APPWRITE_ETFS_COLLECTION_ID=etfs_collection_id
VITE_APPWRITE_PORTFOLIOS_COLLECTION_ID=portfolios_collection_id
VITE_APPWRITE_TRANSACTIONS_COLLECTION_ID=transactions_collection_id
VITE_APPWRITE_DIVIDENDS_COLLECTION_ID=dividends_collection_id

# API Configuration
VITE_POLYGON_API_KEY=your_polygon_api_key_here
VITE_API_BASE_URL=https://api.your-domain.com

# App Configuration
VITE_APP_NAME=Dividend Tracker
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# Security
VITE_ENABLE_ANALYTICS=true
VITE_SENTRY_DSN=your_sentry_dsn_here

# CSP Configuration
VITE_CSP_CONNECT_SRC="'self' https://api.polygon.io wss://socket.polygon.io https://appwrite.your-domain.com"